SELECT ob.*,pos.value FROM lt_ob_zone AS ob INNER JOIN equity e ON (e.id = ob.equity_id AND e.instrument_type = "EQ") INNER JOIN (SELECT td.equity_id, td.value, td.created_at  FROM lt_td_position td WHERE td.id IN (SELECT MAX(id) FROM lt_td_position WHERE created_at < "2026-01-27 10:15:00" AND position = "low" AND deleted_at IS NULL GROUP BY equity_id)) AS pos ON pos.equity_id = ob.equity_id WHERE pos.value BETWEEN (ob.zone_low - (ob.zone_low * 1)/100) AND (ob.zone_high + (ob.zone_high * 1)/100) AND ob.irl_type = "bullish" AND ob.created_at < pos.created_at;